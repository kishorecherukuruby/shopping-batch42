
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>EST</th>
      <th>Country</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @brands.each do |brand| %>
      <tr>
        <td><%= brand.name %></td>
        <td><%= brand.est %></td>
        <td><%= brand.country %></td>
      
      </tr>
    <% end %>
  </tbody>
</table>


<%= link_to "New Brand", new_brand_path %>